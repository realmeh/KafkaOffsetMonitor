{
  "links": {
    "build": "https://bamboo.finn.no/browse/KAFKAOFFSETMONITOR",
    "repository": "https://github.com/finn-no/KafkaOffsetMonitor"
  },
  "versions": {
    "jdk": "1.8"
  },
  "env": {},
  "tasks": {
    "default": [
        {
