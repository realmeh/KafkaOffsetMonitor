        }
    ],
    "deploy": [
          {
              "command": "./sbt clean publish",
              "pipelines": [
                  {
                      "name": "kafkaoffsetmonitor",
                      "pattern": "published:\\s+(http://.+/kafkaoffsetmonitor_(.*).jar)"
                  }
              ]
          }
    ]
  }
}